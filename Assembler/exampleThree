macr athletico
GREAZMAN: add r3, LIST
jmp MOTA
endmacr
endmace
GODIN: prn #48
 lea GREAZMAN, r6
MOTA: .data -3, 67
 inc *r6
 mov #1 , STR
.entry STR
 sub r1, r4
athletico
.extern R9
.extern ROBINHO
 cmp r3, #-6
 add r7, *r6
 clr OBLAK
 sub R9, ROBINHO
.entry LIST	
STR: .string "madrid"
.extern OBLAK
LIST: .data 66, 65, 64
jsr GODIN
